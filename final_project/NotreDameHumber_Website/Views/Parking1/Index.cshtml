@model NotreDameHumber_Website.Models.Parking1
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <title>Add</title>
</head>
<body>
    <div class="container">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)
            <div class="form-group">
                <label for="item_number">item_number</label>
                @Html.TextBoxFor(x => x.item_number, new { @class = "form-control", placeholder = "item_number" })
                @Html.ValidationMessageFor(x=>x.item_number)            
            </div>
            <div class="form-group">
                <label for="item_name">item_name</label>

                @Html.TextBoxFor(x => x.item_name, new { @class = "form-control", placeholder = "item_name" })
                @Html.ValidationMessageFor(x => x.item_name)   
            </div>
            <div class="form-group">
                <label for="amount">amount</label>
                @Html.TextBoxFor(x => x.amount, new { @class = "form-control", placeholder = "amount" })
                @Html.ValidationMessageFor(x => x.amount)   
            </div>
            <div class="form-group">
                <label for="quantity">quantity</label>
                @Html.TextBoxFor(x => x.quantity, new { @class = "form-control", placeholder = "quantity" })
                @Html.ValidationMessageFor(x => x.quantity) 
            </div>

            <button type="submit" class="btn btn-success">Submit</button>

        }
    </div>
    <div class="col-md-12">
        @Html.ActionLink("action link", "Index", new { Controller = "Paypal1" }, new { @class = "btn btn-primary" })
    </div>
</body>
</html>

